{
  "name": "mattdunbar/shopify-app-bundle",
  "type": "symfony-bundle",
  "description": "Shopify App Bundle",
  "keywords": ["shopify", "shopify-app", "symfony", "bundle"],
  "license": "MIT",
  "authors": [
    {
      "name": "Matt Dunbar",
      "email": "matt@mattdsworld.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "symfony/dependency-injection": "7.0.*",
    "symfony/yaml": "7.0.*",
    "symfony/config": "7.0.*",
    "symfony/http-kernel": "7.0.*",
    "symfony/twig-bundle": "7.0.*",
    "symfony/form": "7.0.*",
    "doctrine/annotations": "^2.0",
    "shopify/shopify-api": "^5.2",
    "symfony/validator": "7.0.*",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^2.17",
    "symfony/http-client": "^7.0"
  },
  "require-dev": {
    "phpcsstandards/php_codesniffer": "^3.7",
    "phpro/grumphp-shim": "^2.3",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "7.0.*",
    "symfony/css-selector": "7.0.*",
    "symfony/maker-bundle": "^1.52",
    "symfony/phpunit-bridge": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "MattDunbar\\ShopifyAppBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MattDunbar\\ShopifyAppBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp-shim": true
    }
  }
}
